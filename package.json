    "html-webpack-harddisk-plugin": "0.2.0",
    "html-webpack-plugin": "3.2.0",
    "node-sass": "4.9.0",
    "postcss-load-config": "1.2.0",
    "postcss-loader": "2.1.5",
    "prettier": "1.12.1",
