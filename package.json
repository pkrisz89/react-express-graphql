    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-watch": "2.0.7",
    "copyfiles": "2.0.0",
    "css-loader": "0.28.11",
    "eslint": "4.19.1",
    "eslint-config-fbjs": "2.0.1",
